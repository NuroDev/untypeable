{
  "name": "@untypeable/swapi",
  "version": "1.0.1",
  "description": "Untypeable router type definitions & validators for the swapi Star Wars API",
  "repository": {
    "directory": "packages/swapi",
    "type": "git",
    "url": "https://github.com/nurodev/untypeable.git"
  },
  "homepage": "https://swapi.dev/",
  "bugs": "https://github.com/nurodev/untypeable/issues",
  "author": {
    "name": "nurodev",
    "email": "me@nuro.dev",
    "url": "https://nuro.dev"
  },
  "keywords": [
    "api",
    "swapi",
    "star wars",
    "typescript",
    "untypeable"
  ],
  "license": "MIT",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./zod": {
      "import": "./src/zod.ts",
      "require": "./src/zod.ts",
      "types": "./src/zod.ts"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "zod": [
        "./src/zod.ts"
      ]
    }
  },
  "files": [
    "src/**/*",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:ui": "vitest watch --ui"
  },
  "dependencies": {
    "untypeable": "^0.2.1"
  },
  "devDependencies": {
    "@types/node": "^18.15.3",
    "@vitest/ui": "^0.29.2",
    "dotenv": "^16.0.3",
    "typescript": "^4.9.5",
    "undici": "^5.21.0",
    "vitest": "^0.29.2",
    "zod": "^3.21.4"
  },
  "peerDependencies": {
    "zod": "^3.21.4"
  },
  "peerDependenciesMeta": {
    "zod": {
      "optional": true
    }
  }
}
