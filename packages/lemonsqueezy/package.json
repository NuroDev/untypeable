{
  "name": "@untyped/lemonsqueezy",
  "version": "1.0.1",
  "description": "Untyped API for the official Lemon Squeezy API",
  "repository": {
    "directory": "packages/lemonsqueezy",
    "type": "git",
    "url": "https://github.com/nurodev/untyped.git"
  },
  "homepage": "https://github.com/nurodev/untyped",
  "bugs": "https://github.com/nurodev/untyped/issues",
  "author": {
    "name": "nurodev",
    "email": "me@nuro.dev",
    "url": "https://nuro.dev"
  },
  "keywords": [
    "api",
    "lemonsqueezy",
    "typescript",
    "untyped"
  ],
  "license": "MIT",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./src/index.ts",
      "types": "./src/index.ts"
    },
    "./zod": {
      "import": "./src/zod.ts",
      "require": "./src/zod.ts",
      "types": "./src/zod.ts"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "zod": [
        "./src/zod.ts"
      ]
    }
  },
  "files": [
    "src/**/*",
    "LICENSE",
    "README.md"
  ],
  "devDependencies": {
    "zod": "^3.21.4"
  },
  "peerDependencies": {
    "zod": "^3.21.4"
  },
  "peerDependenciesMeta": {
    "zod": {
      "optional": true
    }
  }
}
